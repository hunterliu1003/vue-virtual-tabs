{
  "name": "vue-virtual-tabs",
  "private": false,
  "version": "0.0.0",
  "source": "src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm run dev:lib & pnpm run dev:docs & pnpm run test",
    "build": "pnpm run build:lib & pnpm run build:docs",
    "dev:lib": "vue-tsc --noEmit && vite build -w",
    "build:lib": "vue-tsc --noEmit && vite build",
    "dev:docs": "vite --config vite.config.docs.ts --host",
    "build:docs": "vite build --config vite.config.docs.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext=.ts,.vue --fix",
    "clean": "pnpm exec -- rm -rf node_modules dist docs coverage .cache",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "pnpm build"
  },
  "dependencies": {
    "@types/scroll-into-view": "^1.16.0",
    "@vueuse/core": "^8.7.5",
    "chroma-js": "^2.4.2",
    "is-what": "^4.1.7",
    "scroll-into-view": "^1.16.2",
    "swiper": "^8.3.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "vue-virtual-scroll-grid": "^1.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/chroma-js": "^2.1.4",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@viteplay/plugin": "^0.0.27",
    "@viteplay/vue": "^0.0.26",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "happy-dom": "^6.0.0",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-dts": "^1.2.0",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.35.0"
  },
  "peerDependencies": {
    "@vueuse/core": "^8.7.5",
    "is-what": "^4.1.7",
    "scroll-into-view": "^1.16.2",
    "swiper": "^8.3.1",
    "vue-virtual-scroll-grid": "^1.7.1"
  }
}